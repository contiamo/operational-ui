{
  "name": "@operational/components",
  "version": "18.0.0",
  "description": "Building blocks for effective operational user interfaces",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "git@github.com:contiamo/operational-ui.git",
  "author": "Contiamo Frontend <frontend@contiamo.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "start": "ts-node -P tsconfig.node.json ./scripts/start.ts",
    "build": "run-s build:*",
    "build:icons": "ts-node -P tsconfig.node.json ./scripts/build-icons.ts -o ./src/Icon",
    "build:package": "rimraf lib .tsbuildinfo && tsc -d",
    "build:types": "ts-node -P tsconfig.node.json ./scripts/generate-type-dictionary.ts -o ./styleguide/__generated__",
    "build:styleguide": "styleguidist build",
    "watch": "tsc -w",
    "test": "run-p test:*",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "preversion": "npm run build:package",
    "version": "auto-changelog -u && git add CHANGELOG.md",
    "precommit": "tsc && lint-staged",
    "ci": "npm run build:icons && npm run test:unit"
  },
  "lint-staged": {
    "*.(js|jsx|json|md)": [
      "prettier --write",
      "git add"
    ],
    "*.(ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "emotion": "^9.2.5",
    "react": "^16.8.0",
    "react-beautiful-dnd": "^11.0.0",
    "react-dom": "^16.8.0",
    "react-emotion": "^9.2.5"
  },
  "jest": {
    "setupFiles": [
      "./test-polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "/.*\\.test\\.(ts|tsx|js|jsx)$",
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "case": "^1.5.5",
    "emotion-theming": "^10.0.10",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "monaco-editor": "^0.17.0",
    "nanoid": "^2.0.1",
    "node-fetch": "^2.3.0",
    "ora": "^3.4.0",
    "qs": "6.6.0",
    "react": "^16.8.1",
    "react-beautiful-dnd": "^11.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.1",
    "react-emotion": "^10.0.0",
    "react-focus-lock": "^2.2.1",
    "react-highlight": "^0.12.0",
    "react-highlight-words": "^0.16.0",
    "react-json-view": "^1.19.1",
    "react-markdown": "4.0.3",
    "react-use-dimensions": "^1.2.1",
    "react-waypoint": "^8.0.3",
    "react-window": "^1.8.1",
    "tinycolor2": "^1.4.1",
    "url-regex": "^4.1.1"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@svgr/core": "^4.3.0",
    "@svgr/plugin-jsx": "^4.3.0",
    "@svgr/plugin-prettier": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "@types/chokidar": "^2.1.3",
    "@types/commander": "^2.12.2",
    "@types/enzyme": "^3.1.12",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.115",
    "@types/nanoid": "^1.2.1",
    "@types/ora": "^3.2.0",
    "@types/progress": "^2.0.3",
    "@types/qs": "6.5.1",
    "@types/react": "^16.8.18",
    "@types/react-beautiful-dnd": "^11.0.0",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-highlight-words": "^0.16.0",
    "@types/react-router": "^4.0.28",
    "@types/react-window": "^1.8.0",
    "@types/tinycolor2": "^1.4.1",
    "@types/url-regex": "^4.1.0",
    "auto-changelog": "^1.13.0",
    "babel-loader": "^8.0.4",
    "base64-img": "^1.0.4",
    "chokidar": "^3.0.1",
    "commander": "^2.20.0",
    "css-loader": "^3.0.0",
    "cypress": "^3.2.0",
    "cypress-plugin-tab": "^1.0.0",
    "danger": "^4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.0.0-beta6",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "husky": "^0.14.3",
    "jest": "^24.3.0",
    "jest-enzyme": "^6.0.2",
    "jest-serializer-enzyme": "^1.0.0",
    "lint-staged": "^7.2.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.13.7",
    "progress": "^2.0.3",
    "react-docgen-typescript": "^1.12.3",
    "react-hooks-testing-library": "^0.5.0",
    "react-styleguidist": "^9.0.1",
    "react-testing-library": "5.6.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "svg-to-dataurl": "^1.0.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.6.0-dev.20190530",
    "webpack": "^4.16.2"
  }
}
